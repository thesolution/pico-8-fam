pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
	cls()
end

--variables
	x=30
	y=35
	r=false
	d=false
	x1=0
	y1=0
	
function _update()
--l&r
	if btnp(1) then
		x=x+2.3
	end
	
	if btnp(0) then
		x=x-2.3
		end
		
--u&d
	if btnp(2) then
		y=y-2.3
	end
	
	if btnp(3) then 
		y=y+2.3
		d=true
	else
		d=false
	end
end
function _draw()
	cls()
	map(0,0,0,0,16,16)
	mset(x,y,5)
	if btnp(1) then
		r=true
	elseif btnp(0) then
		r=false
	end
	
	if d==true then
		if r==true then
			spr(33,x,y,2,2,1)
		else
			spr(33,x,y,2,2)
		end
	else
		if r==true then
			spr(1,x,y,2,2,1)
		else
			spr(1,x,y,2,2)
		end
	end
end

