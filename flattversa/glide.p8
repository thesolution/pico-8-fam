palettes = {}
endless_top = 0
sprint_top = 0
palette_id = 0

function _init()
	cartdata("bluswimmer_glide_1")
	palt(0,false)
	local id = dget(0)
	endless_top = dget(1)
	sprint_top = dget(2)

	title = true
	option = 0
	add_palette(12,1,7,"blue")
	add_palette(14,2,7,"pink")
	add_palette(11,3,7,"green")
	add_palette(6,5,7,"grey")
	
	while(id > 0) do
		swap_palette()
		id -= 1
	end
end

function add_palette(m,s,t,name)
	local palette = {}
	palette.main = m
	palette.sub = s
	palette.text = t
	palette.name = name
	palettes[#palettes+1] = palette
end

function reset()
	hills = {}
	player = {}
	offset_x = 0.25
	up_down = 1
	hill_count = 0
	score = 0
	gameover = false
	wonsprint = false
	timer = 0
	title = false
	music(0)
	
	create_player()
	create_hills()
end

function create_player()
	player.x = 0
	player.y = 64
	player.power = 0
	player.vx = 0
	player.vy = 0
	player.angle = -0.03
end

function create_hills()
	local hill = {}
	hill.height = flr(rnd(15))+5
	hill.offset = 32
	hills[1] = hill
	next_hill()
	next_hill()
	next_hill()
	next_hill()
	next_hill()
end

function invert_hill_gap(hill_count)
	return 127 - hill_gap(hill_count)
end

function hill_gap(hill_count)
	if(sprint) then
		return flr(80 - sqrt(20*(hill_count+1)))
	end
	return flr(80 - sqrt(12*hill_count))
end

function next_hill()
	local hill = {}
	local last = hills[#hills]
	hill.height = flr(rnd(15))+5
	if(hill_count < 6) then
		hill.height = flr(rnd(5))+5
	end
	if(up_down == 1) then
		hill_count += 1
		local gap = invert_hill_gap(hill_count)
		hill.offset = last.offset - last.height + hill.height
 	if(hill.offset + hill.height > gap) then
 		hill.height = ceil((gap-(hill.offset-hill.height))/2)
 		hill.offset =	gap-hill.height
 	end
 else
 	hill.offset = last.offset + last.height - hill.height 	
 	if(hill.offset - hill.height < 0) then
 		hill.height = ceil((hill.height+hill.offset)/2)
 		hill.offset = hill.height
 	end
 end
 up_down *= -1
 hills[#hills+1] = hill
end

function _update60()
	if(not title) then gameloop()
	else menu() end
end
