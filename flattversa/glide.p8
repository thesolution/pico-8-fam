function gameloop()
	if(gameover or wonsprint) then 
		if(btnp(4)) then
			reset()
		elseif(btnp(5)) then
			title = true
		end
		return
	end

	if(btn(2)) then
		player.angle += 0.015
		if(player.angle > 0.24) then player.angle = 0.24 end
	elseif(btn(3)) then
		player.angle -= 0.015
		if(player.angle < -0.24) then player.angle = -0.24 end
	end
	
	handle_flight()

	offset_x+=(player.vx)
	score+=(player.vx*10)
	if(offset_x >= 0.75) then
		offset_x -= 1
		del(hills, hills[1])
		del(hills, hills[1])
		next_hill()
		next_hill()
	end
	
	if(sprint) then
		timer += 1
		if(score >= max_sprint) then
			wonsprint = true
			music(-1)
			sfx(2)
			if(sprint_top == 0 or sprint_top > timer) then
				sprint_top = timer
				dset(2, sprint_top)
			end
			postscores()
		end
	end
	
	collide()
end

function handle_flight()
	if(player.angle < 0) then
		player.power += 0.05 * sin(player.angle)
		player.vx = 0.01*player.power * cos(player.angle)
		player.y += (player.power + player.vy) * sin(player.angle)
	elseif(player.power > 0) then
		player.power += 0.020 * sin(player.angle) - 0.01
		player.vx = 0.01*player.power * cos(player.angle)
		if(player.power < 0.001) then
			player.vx = 0.001 * cos(player.vx)
		end
		player.y += player.power * sin(player.angle)
		if(player.power <= 0) then player.vy = 0 end
	else
		player.vy += 0.01
		player.y += player.vy
	end
end

function collide()
	local hill = hills[1]
	local hill_count = hill_count - 2
	local gap = hill_gap(hill_count)
	local x = 0.015*flr(offset_x/0.015)+0.15
	if(x >= 0.25) then
		hill = hills[2]
	end
	local y = player.y
	if(ceil(hill.height*sin(x)+hill.offset) >= y) then
		lose_game()
	elseif(gap+ceil(hill.height*sin(x)+hill.offset) <= y) then
		lose_game()
	end
end

function lose_game()
	gameover = true
	music(-1)
	sfx(1)
	if(not sprint and endless_top < score) then
		endless_top = flr(score)
		dset(1, endless_top)
	end
	postscores()
end

function postscores()
	set_pin(1,flr(endless_top % 256))
	set_pin(2,flr(endless_top / 256))
	set_pin(3,flr(sprint_top % 256))
	set_pin(4,flr(sprint_top / 256))
	set_pin(0,1)
end

function set_pin(pin,value) --pin : pin between 0 and 127
	poke(0x5f80+pin, value) --value : the value between 0 and 255
end
