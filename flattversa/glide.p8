function menu()
	if(btnp(3)) then
		option = (option + 1) % 3
		sfx(0)
	elseif(btnp(2)) then
		option = (option - 1) % 3
		sfx(0)
	end
	if(btnp(4) or btnp(5)) then
		select_option()
	end
end

function select_option()
	if(option == 0) then
		sprint = false
		reset()
	elseif(option == 1) then
		sprint = true
		max_sprint = 500
		reset()
	else
		swap_palette()
		sfx(0)
	end
end

function swap_palette()
	local palette = (palettes[1])
	palettes[#palettes+1] = palette
	del(palettes, palette)
	
	palette_id = (palette_id+1)%4
	dset(0,palette_id)
end
