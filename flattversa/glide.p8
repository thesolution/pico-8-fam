function swap_colors()
	local palette = palettes[1]
	pal(0,palette.main,1)
	pal(1,palette.sub,1)
	pal(2,palette.text,1)
end

function center_text(s,y)
	print(s,(65-#s*2),y,2)
end

function drawtitle()
	local highlight = {2,2,2}
	highlight[option+1] = 0
	sspr(0,0,24,8,45,41,48,16)
	print("endless",51,70,highlight[1])
	print("sprint",53,76,highlight[2])
	print("swap palette",41,82,highlight[3])
	center_text("made by bluswimmer", 121)

	if(option == 0) then
		center_text("endless best: "..endless_top,53)
	elseif(option == 1) then
		center_text("sprint best: "..timer_string(sprint_top),53)
	else
		center_text("color: "..palettes[1].name,53)
	end
end

function timer_string(t)
	local	seconds = flr(t / 60)
	local ms = flr(t % 60 * 1.66)
	if(ms < 10) return seconds .. ".0" .. ms
	return seconds .. "." .. ms
end

function drawsprintscore()
	local progress = flr(score/max_sprint*100)
	print(timer_string(timer), 1, 1, 2)
	print(progress.."%", 1, 7, 2)
end

function drawendlessscore()
	print(flr(score), 1, 1, 2)
end

function drawgame()
	local x = 0
	local dx = 0.015*flr(offset_x/0.015)
	local hill = hills[1]
	local hill_index = 1
	local hill_count = hill_count - 2
	local gap = hill_gap(hill_count)
	while(x < 128) do
		if(hill_index % 2 == 1 and dx >= 0.25) then
		  hill_index+=1
		  hill = hills[hill_index]
	 elseif(dx >= 0.75) then
	 	hill_index+=1
	 	hill = hills[hill_index]
	 	dx = -0.24
	 	hill_count += 1
	 	gap = hill_gap(hill_count)
	 end
		line(x,0,x,ceil(hill.height*sin(dx)+hill.offset),0)
		line(x,128,x,gap+ceil(hill.height*sin(dx)+hill.offset),0)
		x+=1
		dx+=0.015
	end
	
	line(-4*cos(player.angle)+10,-4*sin(player.angle)+player.y,4*cos(player.angle)+10,4*sin(player.angle)+player.y,0)
	
	if(sprint) then
		drawsprintscore()
	else
		drawendlessscore()
	end

	if(gameover) then
		print("game over",47,56,2)
		print("ðŸ…¾ï¸ to restart",39,62,2)
		print("âŽ to quit",45,68,2)
	elseif(wonsprint) then
		print("you won",51,56,2)
		print("ðŸ…¾ï¸ to restart",39,62,2)
		print("âŽ to quit",45,68,2)
	end
end

function _draw()
	swap_colors()
	cls(1)
	if(title) then
		drawtitle()
		return
	end
	drawgame()
end
